{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/lib/QtCore.framework/Headers",
                "/opt/homebrew/lib/QtGui.framework/Headers",
                "/opt/homebrew/lib/QtWidgets.framework/Headers",
                "/opt/homebrew/lib/QtMultimedia.framework/Headers",
                "/opt/homebrew/include"
            ],
            "defines": [
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_MULTIMEDIA_LIB"
            ],
            "macFrameworkPath": [
                "/opt/homebrew/lib"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}

